@model List<EEE.Domain.Entities.Problem>

<h3>Evlat Edinilen Engeller</h3>

<div class="table-responsive">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Kim</th>
                <th>Ne</th>
                <th>Nerede</th>
                <th>Ne Zaman</th>
            </tr>
        </thead>
        @if (Model != null)
        {
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                        <td>@Model[i].WhoName</td>
                        <td><a href="/problem/detail/@Model[i].IdStr" target="_blank">@Model[i].What</a></td>
                        <td>@Model[i].Where</td>
                        <td>@Model[i].When</td>
                    </tr>
                }
            </tbody>
        }
    </table>
</div>